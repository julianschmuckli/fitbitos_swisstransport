        </use>

        <!-- Have enough dots for your slides (extras are hidden) -->
        <use id='pagination-dots' class='pagination' href='#pagination-widget' y='8'>
          <use href='#pagination-dot'/>
          <use href='#pagination-dot'/>
          <use href='#pagination-dot'/>
          <use href='#pagination-dot'/>
          <use href='#pagination-dot'/>
          <use href='#pagination-highlight-dot'/>
        </use>
        
      </use>
    </symbol>
